{
  "name": "Chrome Serial Monitor",
  "description": "Show one line of serial. Yay!",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js",  "jquery-2.1.4.min.js"]
    }
  },
  "icons": { 
      "32": "img/icon32.png", 
      "48": "img/icon48.png", 
      "64": "img/icon64.png", 
      "128": "img/icon128.png" 
  },
  "permissions": [
      "serial",
      "fullscreen"
  ]
}
